{
  "name": "M1-Transcoder",
  "productName": "M1-Transcoder",
  "version": "1.3.9",
  "description": "Mach1 Spatial layback/conversion application",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "package-mac": "electron-builder",
    "package-win": "electron-builder --x64"
  },
  "osx-sign": {
    "identity": "Developer ID Application: Drazen Bosnjak (6ZETDT84RB)"
  },
  "build": {
    "npmRebuild": "false",
    "appId": "com.mach1.transcoder",
    "mac": {
      "icon": "../resources/icon.icns",
      "asar": true,
      "target": "dir",
      "category": "public.app-category.utilities",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./entitlements.mac.plist",
      "entitlementsInherit": "./entitlements.mac.plist"
    },
    "win": {
      "icon": "/app/assets/win/icon.ico",
      "asar": true,
      "target": "portable"
    },
    "portable": {
      "unicode": false,
      "artifactName": "M1-Transcoder.exe"
    }
  },
  "repository": "",
  "keywords": [
    "Mach1",
    "Spatial",
    "Transcoder",
    "transcode",
    "spatial audio",
    "surround audio"
  ],
  "author": "Mach1",
  "license": "SEE LICENSE IN mach1-semi-open-license.md",
  "devDependencies": {
    "electron": "^1.8.8",
    "electron-builder": "^21.2.0",
    "electron-notarize": "^1.0.0",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.5"
  },
  "dependencies": {
    "asar": "^2.0.3",
    "electron-download-manager": "^2.3.0",
    "electron-is": "^3.0.0",
    "electron-log": "^3.0.6",
    "extract-zip": "^1.6.7",
    "fibers": "^3.1.1",
    "hazardous": "^0.3.0",
    "mv": "^2.1.1",
    "node-gyp": "^9.3.1",
    "rimraf": "^3.0.0"
  }
}
